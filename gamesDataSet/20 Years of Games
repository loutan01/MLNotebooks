    "#create a dataframe from a series...\n",
    "s1 = pd.Series([1,2])\n",
    "s2 = pd.Series([\"Boris Yeltsin\", \"Mikhail Gorbachev\"])\n",
    "pd.DataFrame([s1,s2])"
