   "source": [
    "reviewCount = rawData[['platform','score']].groupby('platform').count()\n",
    "reviewCount.sort_values('score', ascending = False, inplace = True)\n",
    "reviewCount.columns = ['count']\n",
    "reviewCount"
   ]
