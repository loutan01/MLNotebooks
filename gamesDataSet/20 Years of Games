   "outputs": [],
   "source": [
    "import math\n",
    "def countNums(row):\n",
    "    numCount = 0\n",
    "    for i in row:\n",
    "        if (isinstance(i,int) or isinstance(i, float)) and math.isnan(i) ==  False:\n",
    "            numCount += 1\n",
    "    return numCount\n",
    "            "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 177,
   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "y['numcount'] = y.apply(countNums, axis = 1)"
   ]
